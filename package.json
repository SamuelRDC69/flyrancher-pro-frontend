{
  "name": "flyrancher-pro-frontend",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://SamuelRDC69.github.io/flyrancher-pro-frontend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@wharfkit/session": "^1.4.0",
    "@wharfkit/antelope": "^1.0.13",
    "@wharfkit/wallet-plugin-anchor": "^1.0.0",
    "@wharfkit/web-renderer": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^6.1.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "wax",
    "blockchain",
    "nft",
    "automation",
    "ranchers-land",
    "flyrancher",
    "subscription",
    "defi",
    "gaming"
  ],
  "author": "FlyRancher Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SamuelRDC69/flyrancher-pro-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/SamuelRDC69/flyrancher-pro-frontend/issues"
  },
  "description": "Professional-grade automation frontend for Ranchers Land NFTs with subscription management",
  "engines": {
    "node": ">=16.0.0"
  }
}
